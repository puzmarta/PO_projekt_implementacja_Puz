@model IEnumerable<PO_projekt_implementacja_Puz.Models.FieldOfStudy>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FieldName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FieldAbbreviation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FieldDescription)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AdmissionLimit)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StudyProgram)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StudyPlan)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CourseCards)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AcademicTitle)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DegreeLevel)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Faculty)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StudyForm)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FieldName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FieldAbbreviation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FieldDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AdmissionLimit)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StudyProgram)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StudyPlan)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CourseCards)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AcademicTitle.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DegreeLevel.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Faculty.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StudyForm.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
